NODE_ENV=test
PORT=3001

# Test Database (separate from development)
MONGODB_URI=mongodb://localhost:27017/appmarketplace_test

# Test Secrets (simple for testing)
JWT_SECRET=test-jwt-secret
SESSION_SECRET=test-session-secret
BCRYPT_ROUNDS=4

# Test Configuration
TEST_TIMEOUT=30000
TEST_DB_CLEANUP=true
LOG_LEVEL=error

# Mock all external services in tests
MOCK_GITHUB_API=true
MOCK_STRIPE_API=true
MOCK_EMAIL_SERVICE=true
MOCK_OPENAI_API=true

# Test-specific settings
RATE_LIMIT_MAX_REQUESTS=1000
CORS_ORIGIN=*