{
  "name": "pack-code",
  "version": "1.0.0",
  "description": "PackCode - The marketplace for vibe coders. Build together as a pack.",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install --legacy-peer-deps && GENERATE_SOURCEMAP=false ESLINT_NO_DEV_ERRORS=true npm run build && cd .. && mkdir -p public && cp -r client/build/* public/",
    "build:vercel": "cd client && npm install --legacy-peer-deps && GENERATE_SOURCEMAP=false ESLINT_NO_DEV_ERRORS=true npm run build && cd .. && mkdir -p public && cp -r client/build/* public/",
    "start": "node server/index.js",
    "test": "NODE_ENV=test jest --testPathPattern=server/tests --setupFilesAfterEnv=./server/tests/setup.js",
    "test:watch": "NODE_ENV=test jest --testPathPattern=server/tests --setupFilesAfterEnv=./server/tests/setup.js --watch",
    "test:coverage": "NODE_ENV=test jest --testPathPattern=server/tests --setupFilesAfterEnv=./server/tests/setup.js --coverage",
    "test:client": "cd client && npm test",
    "test:all": "npm test && npm run test:client"
  },
  "keywords": [
    "marketplace",
    "coding",
    "projects",
    "licensing",
    "pack",
    "vibe coders",
    "developer community"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "openai": "^5.11.0",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^13.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.0",
    "jest": "^30.0.5",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
